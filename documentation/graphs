//Landpage

digraph{
node [shape="box",style="filled",color="#FFDA6E"];
Container[color="#EC7D78"];
ChartRenderer[color="#EC7D78"];

Container->NavigationBar;
Container->Landpage->ChartRenderer->SelectBox;
}

| Container
| | NavigationBar
| | Landpage
| | | ChartRenderer
| | | | SelectBox

